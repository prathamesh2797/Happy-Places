<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HappyPlaces" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bright_navy_blue</item>
        <item name="colorPrimaryVariant">@color/indigo_dye</item>
        <item name="colorOnPrimary">@color/blue_sapphire</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dodger_blue</item>
        <item name="colorSecondaryVariant">@color/bright_navy_blue</item>
        <item name="colorOnSecondary">@color/blue_sapphire</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    
    <style name="CustomNoActionBarTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        
    </style>
    
    <style name="CustomToolBarStyle">

        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:colorControlNormal" tools:targetApi="lollipop">#FFFFFF</item>
        <item name="android:statusBarColor" tools:targetApi="l">#00487C</item>




    </style>
</resources>